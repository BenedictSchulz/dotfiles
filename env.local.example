# Deploy configuration (used by deploy.sh and clean-deploy.sh)
# Fill these in after setting up your SSH keys and ~/.ssh/config
#
# DEPLOY_HOST  = the SSH host alias from your ~/.ssh/config
# DEPLOY_PATH  = the remote directory to deploy to
# SITE_DIR     = local directory containing your site project
# CONTENT_SOURCE = local directory with content to sync

export DEPLOY_HOST=""
export DEPLOY_PATH=""
export SITE_DIR=""
export CONTENT_SOURCE=""
